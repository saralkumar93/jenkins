pipeline {
    agent any

    stages {
        stage('A') {
            steps {
                echo 'Hello World'
            }
        }
         stage('B') {
            steps {
                echo 'Hello World'
            }
        }
         stage('C') {
            steps {
                echo 'Hello World'
            }
        }
         stage('D') {
            steps {
                echo 'Hello World'
            }
        }
          stage('E') {
            steps {
                echo 'Hello World'
            }
        }
    }
    post { 
        always { 
            echo 'I will always say Hello again!'
        }
        failure{ 
            echo 'failure!'
        }
        success { 
            echo 'I m success !'
        }
    }
}
